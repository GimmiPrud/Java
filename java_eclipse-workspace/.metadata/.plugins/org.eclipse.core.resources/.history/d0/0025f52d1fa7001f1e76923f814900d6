
public class Coppia <T> { //<T> definisce il tipo generic (generico)
	// possiamo anche definire pi√π tipi 
	
	private T n1; // ora possiamo definire i nostri dati con il tipo dato sulla classe (<T>)
	private T n2;
	
	
	public Coppia(T n1, T n2) {
		super();
		this.n1 = n1;
		this.n2 = n2;
	}
	
	public Coppia() {
		super();
	}
	
	
	public T getN1() {
		return n1;
	}
	public void setN1(T n1) {
		this.n1 = n1;
	}
	public T getN2() {
		return n2;
	}
	public void setN2(T n2) {
		this.n2 = n2;
	}

	@Override
	public String toString() {
		return "Coppia [n1=" + n1 + ", n2=" + n2 + "]";
	}
	
	

}
